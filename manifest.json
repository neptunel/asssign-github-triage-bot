{
    "_metadata": {
        "major_version": 2
    },
    "display_information": {
        "name": "assign-triage-app"
        
    },
    "features": {
        "app_home": {
            "home_tab_enabled": true,
            "messages_tab_enabled": true,
            "messages_tab_read_only_enabled": false
        },
        "bot_user": {
            "display_name": "assign-triage-app",
            "always_online": true
        },
        "shortcuts": [
            {
                "name": "Run sample shortcut",
                "type": "global",
                "callback_id": "sample_shortcut_id",
                "description": "Runs a sample shortcut"
            }
        ],
        "slash_commands": [
            {
                "command": "/sample-command",
                "description": "Runs a sample command",
                "should_escape": false
            }
        ]
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "channels:history",
                "chat:write",
                "commands",
                "channels:manage",
                "groups:write",
                "users:read"
            ]
            

        }
    },
    "settings": {
        "event_subscriptions": {
            "bot_events": [
                "app_home_opened",
                "message.channels"
            ]
        },
        "interactivity": {
            "is_enabled": true
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": true,
        "token_rotation_enabled": false
    },
    "functions": {


        "assignTriageDuty": {
            "title": "Assign Triage Duty",
            "description": "Assigns the person on the rotation to the github triage rotation",
            "input_parameters": {
                "properties": {
                    
                    "channel_id": {
                        "type": "slack#/types/channel_id",
                        "description": "Channel Id"
                    }
                },
                "required": [
                    "channel_id",
                    "user_id"
                ]
            },
            "output_parameters": {
                "properties": {
                    "assignTriageDuty": {
                        "type": "string",
                        "description": "------------"
                    }
                },
                "required": [
                    "assignTriageDuty"
                ]
            }
        },
        "addPerson": {
            "title": "Update Triage Workflow Rotation Add",
            "description": "Updates or changes the current rotation",
            "input_parameters": {
                "properties": {
                    "add_new_person": {
                        "type": "slack#/types/user_id",
                        "description": "Add New Person to the Rotation"
                    }, 
                    "channel_id": {
                        "type": "slack#/types/channel_id",
                        "description": "Channel Id"
                    },
                    "user_id": {
                        "type": "slack#/types/user_id",
                        "description": "User Id"
                    }
                },
                "required": [
                    "add_new_person",
                    "channel_id",
                    "user_id"
                ]
            },
            "output_parameters": {
                "properties": {
                    "addPerson": {
                        "type": "string",
                        "description": "////////////"
                    }
                },
                "required": [
                    "addPerson"
                ]
            }
        },

        "removePerson": {
            "title": "Update Triage Workflow Rotation Remove",
            "description": "Updates or changes the current rotation",
            "input_parameters": {
                "properties": {
                    "removePerson": {
                        "type": "slack#/types/user_id",
                        "description": "Remove a Person From the Rotation"
                    }, 
                    "channel_id": {
                        "type": "slack#/types/channel_id",
                        "description": "Channel Id"
                    },
                    "user_id": {
                        "type": "slack#/types/user_id",
                        "description": "User Id"
                    }
                },
                "required": [
                    "removePerson",
                    "channel_id",
                    "user_id"
                ]
            },
            "output_parameters": {
                "properties": {
                    "removePerson": {
                        "type": "string",
                        "description": "////////////"
                    }
                },
                "required": [
                    "removePerson"
                ]
            }
        },


        "advanceRotationOrder": {
            "title": "Advance Triage Rotation Order",
            "description": "Advances the rotation and creates new order",
            "input_parameters": {
                "properties": {
                    "channel_id": {
                        "type": "slack#/types/channel_id",
                        "description": "Channel Id"
                    },
                    "user_id": {
                        "type": "slack#/types/user_id",
                        "description": "User Id"
                    }
                    
                },
                "required": [
                    "channel_id",
                    "user_id"
                ]
            },
            "output_parameters": {
                "properties": {
                    "advanceRotationOrder": {
                        "type": "string",
                        "description": "***********"
                    }
                },
                "required": [
                    "advanceRotationOrder"
                ]
            }
        }

    },
    "outgoing_domains": []
}